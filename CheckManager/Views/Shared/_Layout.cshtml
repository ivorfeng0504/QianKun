<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - 商品数据核验</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/layui/css/layui.css" />
        @RenderSection("Style", required: false)
    </environment>
</head>
<body class="layui-layout-body" style="background-color: #F2F2F2;">
    <div class="layui-layout layui-layout-admin">
        <div class="layui-header">
            <div class="layui-logo">商品中心仪表盘</div>
            <ul class="layui-nav layui-layout-right">
                <li class="layui-nav-item">
                    <a href="javascript:;">
                        <img style="width: 100px;height: 50px;" src="https://doc.ezrpro.com/static/theme/default/image/ui_helplogo.png" class="layui-nav-img" alt="">开发平台部
                    </a>
                    @*<dl class="layui-nav-child">
                            <dd><a href="">基本资料</a></dd>
                            <dd><a href="">安全设置</a></dd>
                        </dl>*@
                </li>
                @*<li class="layui-nav-item"><a href="">登出</a></li>*@
            </ul>
        </div>

        <div class="layui-side layui-bg-black">
            <div class="layui-side-scroll">
                <ul class="layui-nav layui-nav-tree" lay-filter="test">
                    <li class="layui-nav-item layui-nav-itemed">
                        <a class="" href="javascript:;">所有商品</a>
                        <dl class="layui-nav-child">
                            <dd><a href="javascript:;">列表一</a></dd>
                            <dd><a href="javascript:;">列表二</a></dd>
                            <dd><a href="javascript:;">列表三</a></dd>
                            <dd><a href="">超链接</a></dd>
                        </dl>
                    </li>
                    <li class="layui-nav-item"><a href="">发布商品</a></li>
                </ul>
            </div>
        </div>

        <div id="RenderBody" class="layui-body">
            <div class="layui-fluid" style="padding: 10px 15px 0 15px;">
                @RenderBody()
            </div>
        </div>

        <div class="layui-footer">
            © EZR - 2019
        </div>
    </div>
    上
    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/layui/layui.js"></script>
        <script type="text/javascript">
            layui.use('element');
        </script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
