<?xml version="1.0" encoding="utf-8"?>
<!--sqlmapping key s.表示查询语句；u.表示修改语句；d.表示删除语句；i.表示插入语句；-->
<configuration>
  <sqlmapping key="count.rtl_prod_detail">
    <description>
      时间:2019-05-13
      作者:zer
      功能:查询商品中心商品总数
      参数:无
    </description>
    <sql>
      <![CDATA[
          SELECT COUNT(id) FROM rtl_prod_detail
          WHERE BrandId = @BrandId
          AND IsActive = 1
      ]]>
    </sql>
  </sqlmapping>
</configuration>